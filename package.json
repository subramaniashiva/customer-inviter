  },
  "homepage": "https://github.com/subramaniashiva/customer-inviter#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "mocha": "^8.4.0",
    "nock": "^13.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
